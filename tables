      MKT_CD CHAR(4) CHARACTER SET LATIN NOT CASESPECIFIC COMPRESS ('005 ','00H ','00I ','00J ','00K ','00M ','00N ','00O ','018 ','021 ','023 ','026 ','027 ','029 ','02A ','037 ','039 ','03Z ','040 ','044 ','046 ','048 ','053 ','055 ','057 ','059 ','05A ','05J ','05K ','05L ','05M ','05N ','05P ','05R ','060 ','061 ','063 ','065 ','066 ','06D ','06E ','06F ','06I ','06R ','074 ','075 ','076 ','083 ','086 ','089 ','091 ','111 ','128 ','140 ','159 ','161 ','201 ','209 ','255 ','274 ','287 ','317 ','328 ','329 ','337 ','340 ','341 ','343 ','345 ','347 ','348 ','349 ','351 ','356 ','358 ','362 ','363 ','364 ','365 ','366 ','368 ','369 ','371 ','372 ','373 ','374 ','375 ','377 ','378 ','381 ','382 ','383 ','385 ','388 ','390 ','398 ','402 ','405 ','412 ','415 ','418 ','421 ','422 ','428 ','429 ','430 ','431 ','435 ','439 ','443 ','445 ','448 ','454 ','455 ','456 ','457 ','458 ','459 ','462 ','463 ','464 ','468 ','473 ','489 ','494 ','500 ','506 ','509 ','511 ','515 ','516 ','529 ','530 ','534 ','536 ','538 ','542 ','544 ','545 ','546 ','547 ','548 ','549 ','584 ','589 ','601 ','604 ','605 ','610 ','612 ','613 ','617 ','618 ','619 ','621 ','624 ','625 ','626 ','627 ','628 ','630 ','632 ','635 ','640 ','642 ','645 ','646 ','647 ','648 ','651 ','655 ','656 ','664 ','666 ','667 ','668 ','669 ','670 ','671 ','672 ','681 ','682 ','688 ','692 ','693 ','694 ','699 ','701 ','723 ','726 ','730 ','731 ','732 ','733 ','734 ','735 ','736 ','737 ','738 ','741 ','742 ','744 ','745 ','748 ','749 ','750 ','751 ','753 ','761 ','762 ','763 ','764 ','766 ','768 ','771 ','772 ','773 ','774 ','775 ','786 ','789 ','790 ','796 ','797 ','803 ','808 ','809 ','823 ','824 ','847 ','853 ','889 ','896 ','897 ','939 ','942 ','943 ','945 ','959 ','960 ','961 ','968 ','969 ','971 ','973 ','976 ','977 ','981 ','982 ','983 ','984 ','986 ','987 ','989 '),
