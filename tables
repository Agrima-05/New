CREATE VIEW COE_PRD_ALLVM.PROMO_VBM_XREF_V 
AS LOCKING ROW FOR ACCESS
SELECT * FROM  COE_PRD_TBLS.PROMO_VBM_XREF;

CREATE MULTISET TABLE COE_PRD_TBLS.PROMO_VBM_XREF ,FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT,
     DEFAULT MERGEBLOCKRATIO,
     MAP = TD_MAP2
     (
      OFFER_ID VARCHAR(30) CHARACTER SET LATIN NOT CASESPECIFIC,
      OFFER_EFF_DT DATE FORMAT 'yyyy-mm-dd',
      OFFER_EXP_DT DATE FORMAT 'yyyy-mm-dd',
      CAMPAIGN_TYPE VARCHAR(30) CHARACTER SET LATIN NOT CASESPECIFIC COMPRESS ('CDMA','Equipment','Strategic'))
PRIMARY INDEX ( OFFER_ID ,OFFER_EFF_DT ,OFFER_EXP_DT );

CREATE SET TABLE COE_PRD_TBLS.PROMO_SUM_FACT ,FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT,
     DEFAULT MERGEBLOCKRATIO,
     MAP = TD_MAP2
     (
      RPT_MTH DATE FORMAT 'yyyy-mm-dd',
      PROMO_MTH DATE FORMAT 'yyyy-mm-dd',
      PROMO_DT DATE FORMAT 'yyyy-mm-dd',
      PROMO_MOF INTEGER,
      SOR_ID CHAR(1) CHARACTER SET LATIN NOT CASESPECIFIC,
      CUST_ID VARCHAR(15) CHARACTER SET LATIN NOT CASESPECIFIC,
      CUST_LINE_SEQ_ID VARCHAR(15) CHARACTER SET LATIN NOT CASESPECIFIC,
      ACCT_NUM VARCHAR(20) CHARACTER SET LATIN NOT CASESPECIFIC,
      MTN CHAR(10) CHARACTER SET LATIN NOT CASESPECIFIC,
      ORIG_SOR_ID CHAR(1) CHARACTER SET LATIN NOT CASESPECIFIC,
