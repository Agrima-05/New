CUST_ID ,CUST_LINE_SEQ_ID )
PARTITION BY RANGE_N(RPT_MTH  BETWEEN DATE '2015-01-01' AND DATE '2025-12-31' EACH INTERVAL '1' MONTH ,
 NO RANGE OR UNKNOWN)
INDEX ( RPT_MTH ) ORDER BY VALUES ( RPT_MTH )
INDEX ( CUST_ID ,CUST_LINE_SEQ_ID )
INDEX ( MTN );

show view  COE_PRD_ALLVM.PROMO_ACTIVITY_V;

REPLACE VIEW COE_PRD_ALLVM.PROMO_ACTIVITY_V
AS LOCKING ROW FOR ACCESS
SELECT
X.RPT_MTH,
X.PROMO_DT,
X.PROMO_EFF_TS,
X.SOR_ID,
X.CUST_ID,
X.CUST_LINE_SEQ_ID,
X.ACCT_NUM,
X.MTN,
X.STORE_NUM,
X.INVC_NUM,
X.INVC_LINE_NUM,
CASE
WHEN PROMO_TYPE='GIFT CARD' THEN 'GIFT CARD'
WHEN MARKETING_ID_DESC LIKE '%BOGOHO%' THEN 'BOGOHO'
WHEN MARKETING_ID_DESC LIKE '%BOGO%' THEN 'BOGO'
WHEN MARKETING_ID_DESC LIKE '%BMSM%' THEN 'BMSM'
WHEN MARKETING_ID_DESC LIKE '%TRADE%' THEN 'TRADEIN'
WHEN MARKETING_ID_DESC LIKE '%TRD%' THEN 'TRADEIN'
WHEN MARKETING_ID_DESC LIKE '%GTI%' THEN 'TRADEIN'
ELSE 'FREE MONEY'
END AS PROMO_CATEGORY,
X.PROMO_TYPE,
X.PROMO_DETAIL,
X.SOUR,
CASE WHEN V.OFFER_ID IS NOT NULL THEN 'Y'
WHEN MARKETING_ID_DESC LIKE ANY ( '%LOYALTY%' ,'%CRM_%' ,'%_CRM %','% CRM %')THEN 'Y'
WHEN OFFER_NAME LIKE ANY ('CM%','%LOYALTY%') THEN 'Y'
ELSE 'N' END AS VBM_CAMPAIGN_IND,
X.PROMO_OFFER_NM,
X.SYSTEM_OFFER_NM,
TRIM(OREPLACE(OREPLACE(OREPLACE(OREPLACE(OREPLACE(OREPLACE(OREPLACE(OREPLACE(OFFER_NAME,'ATG',''),'SKI',''),'TOOLBOX',''),'BARCODE OFFER',''),'BAR OFFER',''),'BARCODE',''),'QTR',''),'COMPASS')) AS OFFER_NAME,
X.OFFER_NAME AS OFFER_NAME_DETAIL,
X.MARKETING_ID_DESC,
X.MARKETING_ID_START_DT,
X.MARKETING_ID_END_DT,
X.MARKETING_ID,
X.OFFER_ID,
X.OFFER_GROUP_NUM,
X.OFFER_EFF_DT,
X.SVC_PROD_ID,
X.TRADE_IN_REQUIRED,
X.URL,
X.TRANSACTION_TYPE,
X.SALES_QTY,
X.RETURN_QTY,
ZEROIFNULL(X.SALES_QTY) - ZEROIFNULL(X.RETURN_QTY) AS NET_SALES,
X.CLOSE_DESC,
X.CLOSE_DETAIL,
X.CLOSE_DT,
X.CLOSE_TS,
X.CLOSE_MTH,
X.CLOSE_MOF,
X.CLOSE_QTY,
X.CLOSE_LINE_IN_SVC_CNT,
X.OCC_CD,
X.OCC_DESC,
X.TOT_CREDIT_AMT,
X.TOT_CREDIT_MTH_QTY,
X.MTHLY_CREDIT_AMT,
X.TOT_CREDIT_AMT_ISSUED,
X.TOT_CREDITS_ISSUED,
X.TOT_CREDIT_AMT_REMAINING,
X.TRADE_IN_VALUE,
X.ITEM_COST_AMT,
X.ITEM_PRICE_AMT,
X.NET_ALLOC_AMT,
X.MGR_REP_DISC_AMT,
X.INSTANT_REBATE_AMT,
X.LINE_ACT_DT,
X.ORIG_ACCT_ESTB_DT,
X.LINE_IN_SVC_CNT,
X.SLS_DIST_CHNL_TYPE_CD,
X.CHANNEL_DESC,
X.TRANS_SLS_OUTLET_ID,
X.SLS_PRSN_ID,
X.CURR_CONTRACT_TERM,
X.CNTRCT_EFF_DT,
X.CNTRCT_EXP_DT,
X.CNTRCT_PERIOD,
X.LOAN_NUM,
X.DATA_TIER,
X.DEVICE_GROUPING,
X.MFG_NM,
X.DEVICE_BRAND_NM,
X.DEVICE_PROD_NM,
X.EQP_DEVICE_ID,
'No longer upated. Please use VZ2_SEGMT_CTGRY_DESC' AS CONSUMER_IND,
CONSUMER_DETAIL AS CLIENT_ID,
CASE
WHEN CLIENT_ID LIKE '%ACSS%' THEN 'ACSS'
WHEN CLIENT_ID LIKE ANY ('%NETACE%','%VZWACE%') THEN 'NETACE'
WHEN CLIENT_ID LIKE '%EROES%' THEN 'EROES'
WHEN CLIENT_ID LIKE '%ECARS%' THEN 'ECARS'
WHEN CLIENT_ID LIKE ('VZW-MFA-C%') THEN 'Chat Bot'
WHEN CLIENT_ID LIKE ('%MYBUS%') THEN 'My Business'
WHEN CLIENT_ID = 'VZW-MYBU' THEN 'My Business'
WHEN CLIENT_ID LIKE ('%VZW%IVR%') THEN 'VZW IVR'
WHEN CLIENT_ID LIKE '%DIGITAL-ASSIS%' THEN 'Chat Bot'
WHEN CLIENT_ID LIKE '%ONEPOS%' THEN 'One POS'
WHEN CLIENT_ID LIKE ('%-MF-%') THEN 'My VZ App'
WHEN CLIENT_ID LIKE ANY ('VZW-MFA%','VZW-MVM%') THEN 'MyVZ App'
WHEN CLIENT_ID LIKE ANY ('%-MF-%') THEN 'My VZ App'
WHEN CLIENT_ID LIKE ANY ('%VZWCOM%') THEN 'VZW.Com'
WHEN SLS_DIST_CHNL_TYPE_CD = 'N' THEN 'VZW.Com'
WHEN SLS_DIST_CHNL_TYPE_CD IN ('MM','IN','V','W','R','A','I') THEN 'Agent'
WHEN SLS_DIST_CHNL_TYPE_CD = 'D' THEN 'Direct'
WHEN SLS_DIST_CHNL_TYPE_CD = 'DD' THEN 'Door 2 Door'
WHEN SLS_DIST_CHNL_TYPE_CD = 'H' THEN 'Blackhawk'
WHEN SLS_DIST_CHNL_TYPE_CD = 'M' THEN 'Retail'
WHEN SLS_DIST_CHNL_TYPE_CD = 'T' THEN 'Telesales'
WHEN PROMO_TYPE = 'GIFT CARD' THEN 'Blackhawk'
WHEN SLS_DIST_CHNL_TYPE_CD = 'NC' THEN 'ACSS'
ELSE COALESCE(SLS_DIST_CHNL_TYPE_CD,'Other') END AS CLIENT_DESC,
X.MGD_SEGMT,
X.MKT_CD,
X.DMA,
X.DMA_NAME,
X.DMA_TIER,
X.VZ2_AREA_DESC,
X.VZW_MARKET,
X.REGION,
X.STATE_CD,
X.ZIP5_CD,
X.IN_MKT_DECILE,
X.PROMO_LINK,
X.ORIG_SOR_ID,
X.ORIG_CUST_ID,
X.ORIG_CUST_LINE_SEQ_ID,
X.PROMO_CUST_ID,
X.PROMO_SEQ_ID,
X.VALID_RECORD_IND,
X.PROMO_NOTES,
X.VZ2_SEGMT_CTGRY_DESC,
X.VZ2_SEGMT_CD,
X.VZ2_SEGMT_DESC,
X.DISCONNECT_EVER_DESC,
X.DISCONNECT_EVER_CNT,
X.DISCONNECT_EVER_DT,
X.DISCONNECT_EVER_MOF,
X.RUN_DT
FROM COE_PRD_TBLS.PROMO_ACTIVITY X
LEFT OUTER JOIN COE_PRD_ALLVM.PROMO_VBM_XREF_V V
ON X.OFFER_ID = V.OFFER_ID
AND X.PROMO_DT BETWEEN V.OFFER_EFF_DT AND V.OFFER_EXP_DT
WHERE
COALESCE(X.PROMO_DETAIL,'X') <> 'Buy'
AND COALESCE(RESTATE_DESC,'X') <> 'REMOVED';

