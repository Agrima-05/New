CREATE MULTISET TABLE UDM_PRD_TBLS.EQUIP_PROMO_FACT ,FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT,
     DEFAULT MERGEBLOCKRATIO,
     MAP = TD_MAP2
     (
      RPT_MTH DATE FORMAT 'MM/DD/YYYY',
      PYMNT_DT DATE FORMAT 'YYYY/MM/DD',
      INVC_NUM VARCHAR(15) CHARACTER SET LATIN NOT CASESPECIFIC,
      INVC_LINE_NUM VARCHAR(9) CHARACTER SET LATIN NOT CASESPECIFIC,
      INVC_DT DATE FORMAT 'YYYY/MM/DD',
      STORE_NUM CHAR(7) CHARACTER SET LATIN NOT CASESPECIFIC,
      SLS_DIST_CHNL_TYPE_CD CHAR(3) CHARACTER SET LATIN NOT CASESPECIFIC COMPRESS ('A  ','D  ','I  ','M  ','N  ','NC ','R  '),
      ACQ_RET_IND CHAR(1) CHARACTER SET LATIN NOT CASESPECIFIC,
      SOR_ID CHAR(1) CHARACTER SET LATIN NOT CASESPECIFIC,
      CUST_ID VARCHAR(15) CHARACTER SET LATIN NOT CASESPECIFIC,
      CUST_LINE_SEQ_ID VARCHAR(15) CHARACTER SET LATIN NOT CASESPECIFIC,
      ACCT_NUM VARCHAR(20) CHARACTER SET LATIN NOT CASESPECIFIC COMPRESS ('1','2','3','4','5','6'),
      MTN CHAR(10) CHARACTER SET LATIN NOT CASESPECIFIC,
      EQP_DEVICE_ID VARCHAR(256) CHARACTER SET LATIN NOT CASESPECIFIC COMPRESS ,
      OFFER_ID VARCHAR(6) CHARACTER SET LATIN NOT CASESPECIFIC COMPRESS ('10495','11220','11619','12916','12919','12954','15574','15659','15661','15664','15766','16378','19598','24766','24767','26302','26304','26316','26317','26319','26331','26337','7897','8474','8555','8560','86665','86935','88281','998'),
      OFFER_NM VARCHAR(50) CHARACTER SET LATIN NOT CASESPECIFIC COMPRESS ('2013 NEW 49 99 ACCESSORY BUNDLE W 10 DLRS OFF 4TH','2013 NEW 49 99 ACCESSORY BUNDLE','2013 NEW 69 99 ACCESSORY BUNDLE W 10 DLRS OFF 4TH','2013 NEW 69 99 ACCESSORY BUNDLE','89 99 ACCESSORY BUNDLE W 10 DLRS OFF 4TH','89 99 ACCESSORY BUNDLE','ACC BUY 5 GET 30 PERCENT OFF UP TO 20 DOLLARS EACH','ACC OFFER 69 99 ACCESSORY BUNDLE','ACC OFFER BUY 3 ACC GET 20 PRCNT OR 20 DOLLARS OFF','ACC OFFER IPHONE POWER PROTECTION PACKAGE','ACC OFFER SMT PHN BUNDLE SAVE 5 ON CLA CAS AND SCR','ACC OFFER SMT PHN BUNDLE SAVE 5 TO 10 ON BUNDLES','ACC OFFER SMTPHN BNDL 3 OFF SCR 5 OFF CAS AND CLA','ACC OFFER SMTPHN BNDL 5 OFF CAS AND SCR 10 OFF CLA','ACCESSORY BUNDLE BUY 2 OR MORE GET 25 PERCENT OFF','ACCESSORY BUNDLE BUY 3 OR MORE GET 25 PERCENT OFF','ACCESSORY BUNDLE BUY 3 OR MORE GET 25 PRCNT OR 20','ACCESSORY BUSINESS CHANNEL B2B TIERED OFFER','ACCESSORY ESSENTIAL PACKAGE','BUY 2 ACCESSORIES GET 20 PCT OFF','CM OFFER EARLY EDGE SMT TRADE IN T12 UPGRADE SA','NE2 CORRECTIONS','NEW EVERY TWO TIERED','SAVE 50 PERCENT OFF SMARTPHONES ACT UPG T2 2YR','standalone drc replacement - 100 NE2','standalone drc replacement - 50 NE2','standalone drc replacement - online discount'),
