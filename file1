CREATE SET TABLE udm_prd_tbls.IVR_CALL ,FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT,
     DEFAULT MERGEBLOCKRATIO,
     MAP = TD_MAP2
     (
      IVR_CALL_ID VARCHAR(10) CHARACTER SET LATIN CASESPECIFIC,
      DNIS_CD VARCHAR(4) CHARACTER SET LATIN NOT CASESPECIFIC,
      MTN VARCHAR(256) CHARACTER SET LATIN NOT CASESPECIFIC,
      SOR_ID CHAR(1) CHARACTER SET LATIN NOT CASESPECIFIC,
      CUST_ID VARCHAR(15) CHARACTER SET LATIN NOT CASESPECIFIC COMPRESS ,
      CUST_LINE_SEQ_ID VARCHAR(15) CHARACTER SET LATIN NOT CASESPECIFIC COMPRESS ('1','10','11','2','3','4','5','6','7','8','9'),
      CPS_ID VARCHAR(3) CHARACTER SET LATIN NOT CASESPECIFIC COMPRESS ('002','003','004','010','011','012','013','014','015','016','017','018','019','020','021','022','023','024','025','026','027','028','029','030','031','032','033','034','035','036','037','038','039','040','041','042','043','044','045','046','047','048','049','050','051','052','053','054','055','056','057','058','060','061','062','063','064','065','066','067','068','069','070','071','072','073','074','075','076','077','078','079','080','081','082','083','084','085','086','087','088','089','106','107','108','109','110','111','112','113','114','115','116','117','118','119','120','121','122','123','124'),
